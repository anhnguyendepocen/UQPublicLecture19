   "source": [
    "import numpy as np\n",
    "from numpy import pi\n",
    "\n",
    "c = (2*pi)**(1/2)\n",
    "H = lambda x: c*np.sqrt(np.abs(x))\n",
    "N = 10**6\n",
    "x = np.random.randn(N)\n",
    "y = H(x)\n",
    "mY = np.mean(y)\n",
    "print('Estimate = {:3.3f}'.format(mY))"
   ]
  }
