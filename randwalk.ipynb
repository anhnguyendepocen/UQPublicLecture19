   "source": [
    "from numpy.random import rand\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "n =1000\n",
    "p = 0.5\n",
    "u = rand(n)\n",
    "x = (u<p).astype(int)\n",
    "y = 2*x - 1\n",
    "y = np.concatenate(([0],y))\n",
    "z = np.cumsum(y)\n",
    "plt.plot(np.arange(n+1),z)\n",
    "plt.show()"
   ]
